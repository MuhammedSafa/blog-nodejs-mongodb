<%- include('partials/header')%>
<h1>Compose</h1>
<form action="/compose" method="POST">
    <div class="form-group">
      <label for="exampleInputEmail1">Title</label>
      <input type="text" class="form-control" id="exampleInputEmail1" aria-describedby="emailHelp" placeholder="Enter title" name="postTitle">
      <small id="emailHelp" class="form-text text-muted">We'll never share your email with anyone else.</small>
    </div>
    <div class="form-group">
        <label for="exampleFormControlTextarea1">Post</label>
        <textarea class="form-control" id="exampleFormControlTextarea1" rows="5" name="postContent"></textarea>
      </div>
    
    <button type="submit" class="btn btn-primary">Submit</button>
  </form>

<%- include('partials/footer')%>